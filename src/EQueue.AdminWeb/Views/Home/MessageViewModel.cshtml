@model EQueue.AdminWeb.Models.MessageViewModel
@{
    ViewBag.Title = "消息详情";
}
<div>
    @using (Html.BeginForm("Message", "Home", new { }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        <div class="form-group">
            @Html.LabelFor(m => m.MessageOffset, new { @class = "control-label" })
            @Html.TextBoxFor(m => m.MessageOffset, new { @class = "form-control", @style = "display:inline-block" })
            <input type="submit" value="查询" class="btn btn-default" />
        </div>
    }

    <form class="form-horizontal" role="form">
        <div class="form-group">
            <label class="col-sm-2 control-label">MessageOffset</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.MessageOffset, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">Topic</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.Topic, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">QueueId</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.QueueId, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">QueueOffset</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.QueueOffset, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">Code</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.Code, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">StoredTime</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.Message.StoredTime, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">Body</label>
            <div class="col-sm-10">
                @Html.TextBoxFor(m => m.MessageContent, new { @class = "form-control", @style = "display:inline-block", @disabled = "disabled" })
            </div>
        </div>
    </form>
</div>
